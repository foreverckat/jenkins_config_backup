    <hudson.tasks.Shell>
      <command>export JYTHON_HOME=/opt/jython2.7
export PATH=$JYTHON_HOME:$JYTHON_HOME/bin:$PATH  
jybot --pythonpath robotstress/robot/rob.jar --outputdir output\
